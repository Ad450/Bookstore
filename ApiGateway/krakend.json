{
    "version": 3,
    "name": "Bookstore-Gateway",
    "timeout": "3000ms",
    "cache_ttl": "300s",
    "output_encoding": "json",
    "port": 9000,
    "endpoints": [
      
      {
        "endpoint": "/login",
        "method": "GET",
        "output_encoding": "json",
        "backend": [
          {
            "url_pattern": "/v1/books",
            "encoding": "json",
            "method": "GET",
            "host": [
              "http://localhost:8083"
            ]
          }
        ],
        "extra_config": {
          "auth/validator": {
            "alg": "HS256",
            "jwk_url": "https://github.com/Ad450/Bookstore/blob/main/AuthService/src/symmetric.json",
            "k":"and0LXNlcmNyZXQ=",
            "cache": true
        }
        }
      },
       
      {
        "endpoint": "/v1/books",
        "method": "GET",
        "output_encoding": "json",
        "backend": [
          {
            "url_pattern": "/v1/books",
            "encoding": "json",
            "method": "GET",
            "host": [
              "http://localhost:8083"
            ]
          }
        ]
       
      }
     
    ]
  }