{
    "$schema": "https://www.krakend.io/schema/v3.json",
    "version": 3,
    "name": "Bookstore-Gateway",
    "timeout": "3000ms",
    "cache_ttl": "300s",
    "output_encoding": "json",
    "port": 9000,
    "endpoints": [
      {
        "endpoint": "/v1/books",
        "method": "GET",
        "output_encoding": "json",
        "backend": [
          {
            "url_pattern": "/v1/books",
            "encoding": "json",
            "method": "GET",
            "host": [
              "http://host.docker.internal:8000"
            ]
          }
        ]
      }
    ]
  }